
public class Assignment {

	public static void main(String[] args) {
		
		int prime,n;
		System.out.println("All prime numbers between 1 to 100");
		for(int i=2;i<=100;i++) {
			prime=0;
			for(int j=2;j<=i/2;j++) {
				
				if(i%j==0) {
					prime=1;
					break;
					
				}
			}
			if(prime==0) {
				System.out.println(i + " ");
			}
			
		}

	}

}
