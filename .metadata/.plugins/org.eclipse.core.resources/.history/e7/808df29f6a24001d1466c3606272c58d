package com.AttraNestedClass;

public class Anonymous {

	public static void main(String[] args) {
		int data=5;
		System.out.println("starting the program execution");
		
		Ekart ekart=new Ekart() {
			void addItem() {
				System.out.println(data + "item added to the ekart are..");
			}
			
		};
		System.out.println("Invoking the anonymous class from the outer");
		ekart.addItom();
		System.out.println("program transmitted....");
		

	}

}
